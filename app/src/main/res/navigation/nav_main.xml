<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="org.android.turnaround.presentation.home.HomeFragment"
        android:label="HomeFragment"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_todoEventFragment"
            app:destination="@id/todoEventFragment" />
    </fragment>
    <fragment
        android:id="@+id/roomFragment"
        android:name="org.android.turnaround.presentation.room.RoomFragment"
        android:label="RoomFragment"
        tools:layout="@layout/fragment_room" />
    <fragment
        android:id="@+id/activityFragment"
        android:name="org.android.turnaround.presentation.activity.ActivityFragment"
        android:label="ActivityFragment"
        tools:layout="@layout/fragment_activity" />
    <fragment
        android:id="@+id/myFragment"
        android:name="org.android.turnaround.presentation.my.MyFragment"
        android:label="MyFragment"
        tools:layout="@layout/fragment_my" />
    <fragment
        android:id="@+id/todoEventFragment"
        android:name="org.android.turnaround.presentation.todoevent.TodoEventFragment"
        android:label="TodoEventFragment"
        tools:layout="@layout/fragment_todo_event"/>
</navigation>
